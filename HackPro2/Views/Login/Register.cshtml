
@{
    ViewBag.Title = "Register";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>HackPro Register</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
    <!-- CSS Libs -->
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/iCheck/skins/flat/_all.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/DataTables/media/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/bower_components/select2/dist/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/vendor/css/dataTables.bootstrap.css">
    <!-- CSS App -->
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/flat-admin-bootstrap-templates-master/css/themes.css">
</head>
<body class="flat-blue login-page Rbody">
    <div class="container">
        <div class="login-box">
            <div>
                <div class="login-form row">
                    <div class="col-sm-12 text-center login-header">
                        <i class="login-logo fa fa-connectdevelop fa-5x"></i>
                        <h4 class="login-title">HackPro</h4>
                        <h5 class="login-title">Register</h5>
                    </div>
                    <div class="col-sm-12">
                        <div class="login-body">
                            <div class="progress hidden" id="login-progress">
                                <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                                    Creando...
                                </div>
                            </div>
                            <form>
                                <div class="control">
                                    <span class="fa fa-at" aria-hidden="true"><strong> Correo Electronico</strong></span>
                                    <input type="text" class="form-control" value="Correo" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-user" aria-hidden="true"><strong> Nombres</strong></span>
                                    <input type="text" class="form-control" value="1er Nombre" />
                                </div>
                                <div class="control">
                                    <input type="text" class="form-control" value="2do Nombre" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-user" aria-hidden="true"><strong> Apellido</strong></span>
                                    <input type="text" class="form-control" value="1er Apellido" />
                                </div>
                                <div class="control">
                                    <input type="text" class="form-control" value="2do Apellido" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-calendar" aria-hidden="true"><strong> Fecha Nacimiento</strong></span>
                                    <input type="text" class="form-control" value="31/12/1994" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-phone" aria-hidden="true"><strong> Telefono</strong></span>
                                    <input type="text" class="form-control" value="87654321" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-venus-mars" aria-hidden="true"><strong> Genero</strong></span>
                                    <input type="text" class="form-control" value="Genero" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-shield" aria-hidden="true"><strong> Contraseña</strong></span>
                                    <input type="password" class="form-control" value="Password" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-shield" aria-hidden="true"><strong> Confirmar Contraseña</strong></span>
                                    <input type="password" class="form-control" value="Password" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-user" aria-hidden="true"><strong> Nombre De Usuario</strong></span>
                                    <input type="text" class="form-control" value="Username" />
                                </div>
                                <div class="control">
                                    <span class="fa fa-suitcase" aria-hidden="true"><strong> Ocupacion</strong></span>
                                    <input type="text" class="form-control" value="Ocupacion" />
                                </div>

                                <div class="login-button text-center">
                                    <input type="submit" class="btn btn-primary" value="Crear">
                                </div>
                            </form>
                        </div>
                        <div class="login-footer">
                            <span class="text-right"><a href="#" class="color-white">Ya Tienes Una Cuenta</a></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Javascript Libs -->
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/chartjs/Chart.min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/bootstrap-switch/dist/js/bootstrap-switch.min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/iCheck/icheck.min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/matchHeight/jquery.matchHeight-min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/DataTables/media/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/bower_components/select2/dist/js/select2.full.min.js"></script>

    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/vendor/js/dataTables.bootstrap.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/vendor/js/ace/ace.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/vendor/js/ace/mode-html.js"></script>
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/vendor/js/ace/theme-github.js"></script>
    <!-- Javascript -->
    <script type="text/javascript" src="~/Content/flat-admin-bootstrap-templates-master/js/app.js"></script>
</body>
</html>

